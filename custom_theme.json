{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#21c7a8",
          "foreground": "#011627",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ if .WSL }}WSL at {{ end }}{{ .Icon }} {{ .HostName }} ",
          "trailing_diamond": "\ue0b0",
          "type": "os",
          "properties": {
            "windows": "\ue62a"
          }
        },
        {
          "background": "#82AAFF",
          "foreground": "#011627",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf07c",
            "folder_separator_icon": "\\",
            "home_icon": "\ueb06",
            "style": "folder"
          },
          "style": "powerline",
          "template": "<#f2f200> \ue5ff </>{{ path .Path .Location }} ",
          "type": "path"
        },
        {
          "background": "#addb67",
          "foreground": "#011627",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": " <#ffffff>\ue0a0 </>",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "type": "git"
        },
        {
          "background": "#575656",
          "foreground": "#d6deeb",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node",
          "properties": {
            "npm_icon": "<#cc3a3a>\ue71e</>",
            "yarn_icon": "<#348cba>\ue6a7</>",
            "fetch_package_manager": true
          }
        },
        {
          "background": "#575656",
          "foreground": "#d6deeb",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true,
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#234d70",
          "foreground": "#d6deeb",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "time_format": "3:04 PM"
          },
          "style": "diamond",
          "template": " \uf017 {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background": "#2e9599",
          "background_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}\uf421{{ else }}\uf469{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#FFD700",
          "background": "#8f43f3",
          "properties": {
            "display_virtual_env": true,
            "display_default": true,
            "display_version": false,
            "home_enabled": true,
            "prefix": "\uF084 ",
            "postfix": " "
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ef5350",
          "style": "plain",
          "template": "\ue3bf ",
          "type": "root"
        },
        {
          "foreground": "#CD4277",
          "style": "plain",
          "template": " <#45F1C2><b>\u26a1</b></><b>{{ .UserName }}</b> <#26C6DA>\u276f</><#45F1C2>\u276f</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
